<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.acadgild.sagar.todoproject" >

    <uses-sdk android:minSdkVersion="15"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/ToDoAppTheme" >
        <activity
            android:name="com.example.acadgild.sagar.todoproject.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.acadgild.sagar.todoproject.SubActivity"
            android:label="@string/title_activity_sub" >
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value="com.example.acadgild.sagar.todoproject.MainActivity" />
        </activity>
        <activity android:name=".CutomDialog"></activity>
        <activity
            android:name=".ToDoActivity"
            android:label="TD">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            </activity>
    </application>

</manifest>
